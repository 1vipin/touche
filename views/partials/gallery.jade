      h2 Gallery
      hr
      p Lorem ipsum dolor sit amet, consectetur adipiscing elit duis sed.
  .container
    .row
      .categories
        ul.cat
          li
            ol.type
              li
                a.active(href='#', data-filter='*') All
              li
                a(href='#', data-filter='.breakfast') Breakfast
              li
                a(href='#', data-filter='.lunch') Lunch
              li
                a(href='#', data-filter='.dinner') Dinner
